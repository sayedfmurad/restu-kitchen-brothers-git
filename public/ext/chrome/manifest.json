{
  "manifest_version": 3,
  "name": "Telegram Notification",
  "version": "1.0",
  "description": "Notify user of new messages on Telegram web",
  "permissions": ["notifications", "activeTab"],  
  "content_scripts": [
    {
      "matches": ["https://web.telegram.org/*"],
      "js": ["contentScript.js"]
    }
  ]
}
